<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>Gap</display-name>
 <servlet>
    <servlet-name>GetGoodsServer</servlet-name>
    <servlet-class>com.union.GetGoodsServer</servlet-class>
  </servlet>
 
 <servlet>
    <servlet-name>ControlGoodsInfoServer</servlet-name>
    <servlet-class>com.union.ControlGoodsInfoServer</servlet-class>
  </servlet>
  
  <servlet-mapping>
    <servlet-name>ControlGoodsInfoServer</servlet-name>
    <url-pattern>/ControlGoodsInfoServer</url-pattern>
  </servlet-mapping>
 
  
  <servlet-mapping>
    <servlet-name>GetGoodsServer</servlet-name>
    <url-pattern>/GetGoodsServer</url-pattern>
  </servlet-mapping>
  
  
  
</web-app>